{
  "name": "pdfn",
  "version": "0.6.4",
  "description": "Dev server and PDF generation for print-ready PDFs",
  "type": "module",
  "bin": {
    "pdfn": "./dist/cli.js"
  },
  "exports": {},
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist coverage"
  },
  "files": [
    "dist",
    "templates",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "keywords": [
    "pdf",
    "cli",
    "puppeteer",
    "print",
    "document",
    "react",
    "server",
    "generation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pdfnjs/pdfn.git",
    "directory": "packages/cli"
  },
  "homepage": "https://pdfn.dev",
  "author": "gokulsiva (https://github.com/gokulsiva)",
  "bugs": {
    "url": "https://github.com/pdfnjs/pdfn/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@pdfn/client": "workspace:*",
    "@pdfn/react": "workspace:*",
    "@pdfn/vite": "workspace:*",
    "chalk": "^5.6.2",
    "chokidar": "^5.0.0",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "ora": "^9.0.0",
    "puppeteer": "^24.35.0",
    "vite": "^7.3.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@pdfn/eslint-config": "workspace:*",
    "@pdfn/typescript-config": "workspace:*",
    "@types/express": "^5.0.6",
    "@types/react": "^19.2.8",
    "@types/ws": "^8.18.1",
    "pdf-lib": "^1.17.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
