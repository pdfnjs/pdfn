{
  "name": "@pdfx-dev/cli",
  "version": "0.0.1-alpha.1",
  "description": "CLI for PDFX - dev server and PDF generation",
  "type": "module",
  "bin": {
    "pdfx": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/server/index.d.ts",
        "default": "./dist/server/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf dist coverage"
  },
  "files": [
    "dist",
    "templates",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "keywords": [
    "pdf",
    "cli",
    "puppeteer",
    "print",
    "document"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pdfx-dev/pdfx.git",
    "directory": "packages/cli"
  },
  "dependencies": {
    "@pdfx-dev/react": "workspace:*",
    "chalk": "^5.6.2",
    "chokidar": "^5.0.0",
    "commander": "^14.0.2",
    "express": "^5.2.1",
    "ora": "^9.0.0",
    "puppeteer": "^24.33.0",
    "vite": "^7.3.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@pdfx-dev/eslint-config": "workspace:*",
    "@pdfx-dev/typescript-config": "workspace:*",
    "@types/express": "^5.0.6",
    "@types/react": "^19.2.7",
    "@types/ws": "^8.18.1",
    "pdf-lib": "^1.17.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
